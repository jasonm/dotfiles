alias g="git"
alias s="git status"
alias ds="git diff --cached"
alias grc="git rebase --continue"
alias b="bundle"
alias be="bundle exec"
alias mkdir='mkdir -p'

alias ...='../..'
alias ....='../../..'
alias .....='../../../..'
alias ......='../../../../..'

alias gadd="git add . ; git add -u . ; git st"

alias chromedev='open /Applications/Google\ Chrome.app --args --disable-web-security'

alias k='kill -9 %1'

alias ll='ls -lAh'
